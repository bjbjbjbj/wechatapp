/**index.wxss**/
swiper.focus{
  height: 340rpx;
}
swiper.focus navigator, swiper.focus view, swiper.focus image{
  height: 100%; width: 100%;
  position: relative;
  display: block;
}
swiper.focus navigator text, swiper.focus view text{
  height: 60rpx;
  background: rgba(0, 0, 0, 0.5);
  font-size: 28rpx; line-height: 60rpx; text-align: center; color: #fff;
  position: absolute; bottom: 0; left: 0; right: 0;
  overflow: hidden;
}

.entra{
  height: 140rpx;
  padding: 20rpx 5rpx; margin: 20rpx 0;
  background: #fff;
  display: flex;
}
.entra navigator{
  height: 140rpx;
  padding: 0 20rpx;
  display: block;
  flex: 1;
}
.entra navigator image{
  width: 88rpx; height: 88rpx;
  margin: 0 auto;
  border-radius: 50%;
  display: block;
}
.entra navigator text{
  padding-top: 10rpx;
  font-size: 32rpx; line-height: 52rpx; text-align: center; color: #333;
  display: block;
}

view.ul{
  margin-top: 20rpx;
  padding-left: 20rpx; margin-bottom: 20rpx;
  background: #fff;
}
view.ul navigator.list{
  height: 160rpx;
  padding: 10rpx 20rpx 0 0;
  position: relative;
  display: block;
}
view.ul navigator.list + navigator.list{
  border-top: 1rpx solid #eee;
}
view.ul navigator.list .title{
  height: 70rpx;
  font-size: 36rpx; line-height: 70rpx; color: #333;
  overflow: hidden;
}
view.ul navigator.list .text{
  height: 76rpx;
  font-size: 28rpx; line-height: 38rpx; color: #777;
  overflow: hidden;
}




